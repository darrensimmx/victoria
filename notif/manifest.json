{
    "name": "Facial Emotion Notifications",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "notifications",
        "storage",
        "alarms",
        "runtime"
    ],
    "action": {
        "default_popup": "facial.html"
    }
}
